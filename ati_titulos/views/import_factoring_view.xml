<?xml version="1.0" encoding="UTF-8"?>
<odoo>

	<record model="ir.ui.view" id="import_factoring_view_form">
		<field name="name">import.factoring.view.form</field>
		<field name="model">import.factoring</field>
		<field name="arch" type="xml">
			<form string="Importar Factoring">
				<header>
					<button name="btn_process" type="object" string="Procesar" states="draft" />
					<field name="state" widget="statusbar" />
				</header>
				<sheet>
					<group>
						<field name="name" />
						<field name="client_file" widget="binary"/>
						<field name="delimiter" />
						<field name="skip_first_line" />
						<field name="client_match" />
					</group>
					<notebook>
						<page string="Contenido" >
							<field name="file_content" readonly="1"/>
						</page>
						<page string="No procesados">
							<field name="not_processed_content" readonly="1"/>
						</page>
                        <page string="Factoring Cargadas">
                                <field name="clientes_creados" readonly="1"/>
                        </page>
					</notebook>
				</sheet>
			</form>
		</field>
	</record>

	<record model="ir.ui.view" id="import_factoring_tree">
		<field name="name">import.factoring.view.tree</field>
		<field name="model">import.factoring</field>
		<field name="arch" type="xml">
		<tree string="Import Factoring">
			<field name="name" />
			<field name="client_file" />
			<field name="state" />
		</tree>
		</field>
	</record>

</odoo>